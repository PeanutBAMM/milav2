{
  "name": "mila",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "preinstall": "node scripts/pre-install-check.js",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepare": "husky",
    "verify-versions": "node -e \"const pkg=require('./package.json');const invalid=[...Object.entries(pkg.dependencies||{}),...Object.entries(pkg.devDependencies||{})].filter(([,v])=>v.includes('^')||v.includes('~'));if(invalid.length){console.error('ERROR: Found version ranges:',invalid.map(([k,v])=>k+':'+v));process.exit(1);}else{console.log('âœ“ All versions are exact');}\"",
    "organize-docs": "node docs/scripts/organize-docs.js"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.3",
    "@hookform/resolvers": "5.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@react-navigation/stack": "6.3.29",
    "@supabase/supabase-js": "2.50.0",
    "@tanstack/react-query": "5.81.2",
    "expo": "51.0.39",
    "expo-dev-client": "4.0.29",
    "expo-secure-store": "13.0.2",
    "expo-status-bar": "1.12.1",
    "expo-updates": "0.25.28",
    "nativewind": "2.0.11",
    "react": "18.2.0",
    "react-hook-form": "7.58.1",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "tailwindcss": "3.3.0",
    "zod": "3.25.67",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@types/react": "18.2.79",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "glob": "10.3.10",
    "husky": "9.1.7",
    "jimp": "0.22.12",
    "lint-staged": "16.1.2",
    "prettier": "3.6.0",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write",
      "npm run organize-docs"
    ]
  },
  "private": true
}
